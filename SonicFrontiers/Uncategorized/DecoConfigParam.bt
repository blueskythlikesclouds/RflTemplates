typedef struct color8 {
    ubyte r;
    ubyte g;
    ubyte b;
    ubyte a;
} color8;

typedef ubyte uint8;

typedef struct
{
    local uint64 p<hidden=true> = FTell();

    FSeek(p + 0);  color8 clientColor;
    FSeek(p + 4);  uint8 windowAlpha;
    FSeek(p + 8);  color8 startGradationGuideColor;
    FSeek(p + 12); color8 endGradationGuideColor;

    FSeek(p + 16);
} DecoConfigParam;

LittleEndian();

FSeek(64);
DecoConfigParam data;
